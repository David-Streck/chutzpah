{
  "Framework": "jasmine",
  "Compile": {
    "Mode": "Executable",
    "Executable": "../compile.bat",
    "Extensions": [ ".ts" ],
    "ExtensionsWithNoOutput": [ ".d.ts" ],
    "UseSourceMaps": true,
    "Paths" : [
      { "SourcePath": "../", "OutputPath": "../" }
    ]
  },
  "References": [
    { "Path": "../src/Scripts/angular.min.js" },
    { "Path": "../src/Scripts/angular-mocks.js" },
    { "Path": "../src", "Includes": [ "*.ts" ], "Excludes": ["*.d.ts"] }
  ],
  "Tests": [{ "Include": "*Tests.ts" }]
}